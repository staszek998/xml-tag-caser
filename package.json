{
  "name": "@staszek998/xml-tag-caser",
  "version": "1.0.3",
  "description": "Command line tool for switching the casing of the XML tags in multiple files at once",
  "keywords": [
    "oclif",
    "oclif cli",
    "cli",
    "glob",
    "xml",
    "xml tag",
    "xml tag casing",
    "casing",
    "kebab-case",
    "kebab case",
    "PascalCase",
    "pascal case",
    "vue",
    "html",
    "vue template",
    "vue component casing",
    "vue tag casing",
    "quasar",
    "quasar component casing",
    "quasar tag casing"
  ],
  "homepage": "https://github.com/staszek998/xml-tag-caser",
  "bugs": "https://github.com/staszek998/xml-tag-caser/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/staszek998/xml-tag-caser"
  },
  "license": "MIT",
  "author": {
    "name": "Stanisław Gregor",
    "email": "stanislaw.gregor@idealign.zgora.pl",
    "url": "https://idealign.zgora.pl"
  },
  "maintainers": [
    {
      "name": "Stanisław Gregor",
      "email": "stanislaw.gregor@idealign.zgora.pl",
      "url": "https://idealign.zgora.pl"
    }
  ],
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "xml-tag-caser": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "yarn run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "husky && yarn run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn run lint",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4.3.10",
    "change-case": "^5.4.4",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/cli-progress": "^3.11.5",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "mocha": "^10",
    "oclif": "^4.8.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "bin": "xml-tag-caser",
    "dirname": "xml-tag-caser",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  }
}
