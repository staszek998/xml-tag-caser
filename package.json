{
  "name": "@staszek998/xml-tag-caser",
  "version": "0.0.0",
  "description": "Command line tool to switching the casing of the XML tags in multiple files at once",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/staszek998/xml-tag-caser",
  "bugs": "https://github.com/staszek998/xml-tag-caser/issues",
  "repository": "staszek998/xml-tag-caser",
  "license": "MIT",
  "author": "IDEALIGN StanisÅ‚aw Gregor",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "",
  "types": "dist/index.d.ts",
  "bin": {
    "xml-tag-caser": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4"
  },
  "devDependencies": {
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/cli-progress": "^3.11.5",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "mocha": "^10",
    "oclif": "^4.1.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "bin": "xml-tag-caser",
    "dirname": "xml-tag-caser",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
